    "jest": "24.9.0",
    "node-sass": "7.0.0",
    "np": "5.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.3.8",
    "ts-jest": "^24.1.0",
    "tslint": "5.20.0",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "3.6.4"
  },
